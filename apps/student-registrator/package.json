{
  "name": "student-registrator",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "npm --prefix server run dev",
    "dev:desktop": "concurrently -k \"npm run dev:server\" \"tauri dev\"",
    "build": "tsc -b && vite build",
    "build:server": "npm --prefix server run build",
    "build:sidecar": "npm --prefix server run build:binary && npm --prefix server run prepare:tauri",
    "build:desktop": "npm run build:server && npm run build:sidecar && tauri build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.app.json --noEmit"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.0",
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.0.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
